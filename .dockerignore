Dockerfile*
docker-compose*.yml
.dockerignore

# CI/CD files
.github/
.gitlab-ci.yml
.travis.yml
Jenkinsfile

# Development scripts
scripts/dev-*
scripts/local-*

# Research directory (if it contains large files)
research/

# Data directory (if it exists)
data/

