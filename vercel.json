{
  "version": 2,
  "name": "flyerflutter-app",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json|woff|woff2))",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "AIzaSyCJg99ZFUlfdh6WX6bQ5sprAF1wvR7ib-w",
    "GOOGLE_PLACES_API_KEY": "AIzaSyCJg99ZFUlfdh6WX6bQ5sprAF1wvR7ib-w"
  },
  "build": {
    "env": {
      "VITE_GOOGLE_API_KEY": "AIzaSyCJg99ZFUlfdh6WX6bQ5sprAF1wvR7ib-w",
      "GOOGLE_API_KEY": "AIzaSyCJg99ZFUlfdh6WX6bQ5sprAF1wvR7ib-w",
      "GOOGLE_PLACES_API_KEY": "AIzaSyCJg99ZFUlfdh6WX6bQ5sprAF1wvR7ib-w"
    }
  }
}